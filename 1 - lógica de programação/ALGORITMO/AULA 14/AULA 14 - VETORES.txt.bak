	
	AULA 14 - VETORES
	
		VARIÁVEIS COMPOSTAS - PARTE 1
		
			DEFINIÇÃO DE VARIÁVEL SIMPLES: 
				VAR 
				N1 : INT
				N2, N3, N4: INT 
				
				//CADA UM, DEFINIDO EXATAMENTE CONFORME ESCRITO
				
				INICIO
				N1 <- 3
				N2 <- 5
				N3 <- 1
				N4 <- 0
				
			DEFINIÇÃO DE VARIÁVEIS COMPOSTAS HOMOGÊNEAS UNIDIMENSIONAIS: 
			
				VAR 
				N1 : INT
				N2, N3, N4: INT 
				
				//MANTENDO AS VARIÁVEIS ANTERIORES ACIMA
				//CHAMADAS "HOMOGÊNEAS", POIS SÃO VARIÁVEIS DE MESMO TIPO
					//DE "UNIDIMENSIONAIS", POIS PRECISAM DE APENAS 1 ENDEREÇO
					//PARA CADA VARIÁVEL ( OU NÚMERO )
									
				//ABAIXO NOVAS VARIÁVEIS, AS COMPOSTAS
				
				
				
				N: VETOR[(1)..(4)] DE INTEIRO
				I: INTEIRO
				
				//TERMOS: VETOR[ .. ] DE INTEIRO
					//GENERALIZANDO PARA X, Y: VETOR [X..Y] DE (TIPO)
				//VARIAVEIS: 1 & 4
				//PARÊNTESES DEVEM SER OMITIDOS
					
				
				INICIO
				
				PARA I <- 1 ATE 4 FACA
					LEIA (N[I])
					
				FIMPARA
				
			DICA PARA ALINHAR CARACTERES: 
			APÓS DECLARADO
			LEIA ("x")
			COLOQUE AO LADO DA VARIÁVEL, :Nº
			//TAL QUE Nº É O NÚMERO DE ESPAÇOS OCUPADOS PELA VARIÁVEL
			RESULTADO QUE SERÁ RESERVADO UM NÚMERO DE ESPAÇOS PARA A VARIÁVEL
			ALINHANDO O INÍCIO DO PRÓXIMO TERMO. 
			
	
	//
	
	2022.01.08 - 1324
	
		PRÁTICA 04: 
		
			VAR
			VET: VETOR[1..4] DE INTEIRO
			I, J, AUX: INTEIRO
			
			INICIO
				PARA I <- 1 ATE 4 FACA
						ESCREVA ("DIGITE UM VALOR: ")
						LEIA(VET[I])
				FIMPARA
				
				PARA I <- 1 ATE 3
					PARA I+1 ATE 4 FACA 
						SE(VET[I] > VET[J]) ENTAO
							AUX <- VET[I]
							VET[I] <- VET[J]
							VET[J] <- AUX
						FIMSE
					FIMPARA
					
				FIMPARA
				
				PARA I <- I ATE 4 FACA 
					ESCREVA("{", VET[I], "}")
				FIMPARA
				
			FIMALGORITMO