	AULA (15)
		PRATICA (2) 
		V@()
			MOSTRAR UMA MATRIZ 3 X 3 E MOSTRAR OS PARES
		
		//DESENVOLVA O ALGORITMO, E 
		//CONFORME AS VARIÁVEIS
		//PROCEDIMENTOS E FUNÇÕES 
		//FOREM SURGINDO 
		//DECLARE-AS 
		
			//HÁ O RECURSO DE ENTRADAS ALEATÓRIAS PARA PREENCHER "LEIA()"
			//NO VISUALG, NO VÍDEO EM 17:00 MOSTRA 
			//ESTÁ NAS FUNÇÕES DA GUIA SUPERIOR 
			
			//PARA A EXIBIÇÃO EM FORMA MATRIZ, 
			//NÃO PODE SER DIGITADO "ESCREVAL" NO "PARA"
			//DEVE SER "ESCREVA"
		
			ALGORITMO "SEM NOME"
			
			VAR 
				
				VALORES: VETOR[1..3, 1..3] DE INTEIRO 
				L, C, TOTPAR: INTEIRO 
			
			INICIO
				
					PARA L <- 1 ATE 3 FACA
							
							PARA C <- 1 ATE 3 FACA 
							
								ESCREVA("DIGITE O VALOR DA POSIÇÃO [", L, ", ", C, "]: ")
								LEIA(VALORES[L,C])
							
							FIMPARA 
							
					FIMPARA 
				
				//PARA EXIBIR NA TELA A MATRIZ 
				//NO "PARA" DE ESCRITA, "ESCREVA(M[L,C]"
				//APÓS O PRIMEIRO PARA, "ESCREVAL()"
				
				ESCREVAL()
				ESCREVAL("MATRIZ: ")
				ESCREVAL("-------------")
				
				TOTPAR <- 0
				
					PARA L <- 1 ATE 3 FACA
							
							PARA C <- 1 ATE 3 FACA 
							
									SE (VALORES[L,C]%2 = 0) ENTAO 
									
				//IDENTIFICADOR DE PARES 
									
										ESCREVA("{", VALORES[L,C]:2, "}")
										
										TOTPAR <- TOTPAR+1
									
									SENAO
								
										ESCREVA(VALORES[L,C])
									
									FIMSE
							
							FIMPARA 
							
						ESCREVAL()
							
					FIMPARA 
			
			ESCREVAL("AO TODO, FORAM DIGITADOS ", TOTPAR, " VALORES PARES. ")
			
			
			FIMALGORITMO
			
			
			//HOUVE UM ERRO 
			//O VETOR TINHA SIDO DECLARADO DE FORMA ERRADA
			//PARA A EXIBIÇÃO EM MATRIZ, NÃO PODE SER DIGITADO "ESCREVAL" NO "PARA"
			//DEVE SER "ESCREVA"
			
V@2137
			
