	AULA (15)
		PRATICA (1.1) 
		V@(1015)
			PREENCHER UMA MATRIZ COM 3X2 COM VALORES VIA TECLADO
		
		//DESENVOLVA O ALGORITMO, E 
		//CONFORME AS VARIÁVEIS
		//PROCEDIMENTOS E FUNÇÕES 
		//FOREM SURGINDO 
		//DECLARE-AS 
		
			//NOTA: 
			//AQUI É MOSTRADO COMO EXIBIR A MATRIZ EM FORMA DE MATRIZ 
			//E NÃO SIMPLESMENTE EM FORMA (LINEAR, OU, CONCATENADA)
			
				//O COMANDO "ESCREVAL()" 
				//POSICIONADO APÓS O FIMPARA QUE EXIBE A MATRIZ, 
				//EXIBE A MATRIZ 
				//EM FORMA DE MATRIZ 
				
			//CASO DESEJE ESPAÇAR OS VALORES PARA UMA EXIBIÇÃO APRIMORADA
			//ADICIONE ":<NÚMERO DE CASAS ALOCADAS POR VALOR>" APÓS 
			//"MAT[L,C]" DO 
			//"PARA" QUE EXIBE A MATRIZ
			
			//A LINHA 53 & 59 EXIBE OS TERMOS ACIMA 
		
			ALGORITMO "SEM NOME"
			
			VAR 
				
				MAT: VETOR[1..3, 1..2] DE INTEIRO
				
				L, C: INTEIRO 
			
			INICIO
				
				PARA L <- 1 ATE 3 FACA 
				
						PARA C <- 1 ATE 2 FACA 
						
							ESCREVA("DIGITE O VALOR DA POSIÇÃO [", L, ", ", C, "]: ")
							LEIA(MAT[L, C])
						
						FIMPARA 
				
				FIMPARA 
				
				PARA L <- 1 ATE 3 FACA 
				
						PARA C <- 1 ATE 2 FACA 
						
							ESCREVA(MAT[L,C]:5)
						
						FIMPARA 
						
						ESCREVAL()
						
						//O COMANDO ACIMA "ESCREVAL()" EXIBE A MATRIZ 
						//EM FORMA DE MATRIZ 
				
				FIMPARA 
			
			FIMALGORITMO
			
			
			
