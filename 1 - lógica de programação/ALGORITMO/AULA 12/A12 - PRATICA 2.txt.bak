	AULA (12)
		PRATICA (2.1) 
		V@(1320)
			VERIFICADOR PAR OU ÍMPAR
		
		//DESENVOLVA O ALGORITMO, E 
		//CONFORME AS VARIÁVEIS FOREM SURGINDO
		//DECLARE-AS
		
		//NOTA: 
		//NEM SEMPRE É POSSÍVEL
		//NOMEAR PARÂMETROS
		//COM OS MESMOS NOMES DE VARIÁVEIS 
		//DO ALGORITMO
		
			ALGORITMO "SEM NOME"
				
			VAR 
				N: INTEIRO
			
					PROCEDIMENTO PAROUIMPAR(V: INTEIRO)
						
					INICIO 
					
						SE (V%2 = 0) ENTAO
							ESCREVAL("O NÚMERO, ", V, " É PAR")
						SENAO 
							ESCREVAL("O NÚMERO, ", V, " É ÍMPAR")
						FIMSE
					
					FIMPROCEDIMENTO 


			INICIO
				
				ESCREVA("DIGITE UM NÚMERO: ")
				LEIA(N)
				
				PAROUIMPAR(N)
			
			FIMALGORITMO
			
			
			
