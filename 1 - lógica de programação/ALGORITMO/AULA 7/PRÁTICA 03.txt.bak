			ALGORITMO "CALCULOIMC"

			VAR 
				M, A, IMC: REAL

			INICIO 
				ESCREVA("MASSA(Kg): ")
				LEIA (M)
				ESCREVA("ALTURA (m): ")
				LEIA (A)
				IMC <- M / (A)^2
				ESCREVAL("IMC: ", IMC:5:2)


			FIMALGORITMO 
			
		
		PRATICA 3.2
		
			ALGORITMO "CALCULOIMC"

			VAR 
				M, A, IMC: REAL

			INICIO 
				ESCREVA("MASSA(Kg): ")
				LEIA (M)
				ESCREVA("ALTURA (m): ")
				LEIA (A)
				IMC <- M / (A)^2
				ESCREVAL("IMC: ", IMC:5:2)
				
